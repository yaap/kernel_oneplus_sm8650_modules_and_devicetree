		reset-names = "pcie_0_core_reset", "pcie_0_phy_reset";

	dma-coherent;
		qcom,smmu-sid-base = <0x1400>;
		iommu-map = <0x0 &apps_smmu 0x1400 0x1>,
					<0x100 &apps_smmu 0x1401 0x1>;

	qcom,boot-option = <0x1>;
	qcom,no-l1ss-supported;
