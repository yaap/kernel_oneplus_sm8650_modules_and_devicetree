	pinctrl-names = "default", "sleep";
	perst-gpio = <&tlmm 66 GPIO_ACTIVE_HIGH>;
	wake-gpio = <&tlmm 68 GPIO_ACTIVE_HIGH>;

	pinctrl-0 = <&pcie0_clkreq_default
